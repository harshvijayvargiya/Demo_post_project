<p id="notice"><%= notice %></p>

<h1>All User Posts</h1>

<%= form_tag( all_user_post_posts_path, :method => "get", id: "search-form") do %>
<%= text_field_tag :search, params[:search], placeholder: "Search Posts" %>
<%= submit_tag "Search" %>
<% end %>

<%= render 'post' %>
<%= paginate @posts %>

